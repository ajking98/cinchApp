source 'https://github.com/CocoaPods/Specs.git'

target 'Cinch' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
  pod 'Firebase/Core'
  pod 'Firebase/Database'
  pod 'SDWebImage/WebP'
  pod 'Firebase/Storage','~>4.0'
  platform :ios, '10.0'
  pod 'SQLite.swift', '~> 0.12.0'
  
  # Pods for CameraViewFinal
  #  pod "EasyImagy", :path => "../swift-image"
  pod 'XLActionController/Spotify'
  
  
  #for Profile page
  pod 'Cards'
  pod 'CardParts'
  
  pod 'IBMMobileFirstPlatformFoundation', '~>8.0.2020011414'
  
  # Pods for Cinch
  
  target 'CinchTests' do
    inherit! :search_paths
    # Pods for testing
  end
  
  target 'CinchUITests' do
    inherit! :search_paths
    # Pods for testing
  end
  
end

target 'iMessage' do
  use_frameworks!
  pod 'Firebase/Core'
  pod 'Firebase/Database'
  pod 'SDWebImage/WebP'
  pod 'Firebase/Storage','~>4.0'
